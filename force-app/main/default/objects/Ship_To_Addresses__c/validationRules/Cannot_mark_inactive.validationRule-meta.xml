<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_mark_inactive</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Inactive__c = TRUE, Preferred_or_Qualifying_Ship_to_Address__c =TRUE)</errorConditionFormula>
    <errorDisplayField>Inactive__c</errorDisplayField>
    <errorMessage>Must Make another record Preferred Address before Inactivating this record.</errorMessage>
</ValidationRule>
