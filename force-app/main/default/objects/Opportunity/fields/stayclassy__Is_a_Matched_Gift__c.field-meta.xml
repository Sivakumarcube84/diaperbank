<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>stayclassy__Is_a_Matched_Gift__c</fullName>
    <deprecated>false</deprecated>
    <description>Indicates if the Opportunity is a matched gift on the Classy platform.</description>
    <formula>IF(stayclassy__Parent_Transaction_ID__c &gt; 0, true, false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Is a Matched Gift?</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
